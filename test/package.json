{
  "name": "test-environment",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "npm run watch-once; vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "watch": "npx babel -D --source-maps inline --out-dir selenium --no-babelrc --presets=es2015 --watch selenium-src",
    "watch-once": "npx babel -D --source-maps inline --out-dir selenium --no-babelrc --presets=es2015 selenium-src "
  },
  "dependencies": {
    "axios": "^0.18.0",
    "core-js": "^2.6.5",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "chromedriver": "^2.46.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "vue-cli-plugin-scss-base": "^0.1.10",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
